    "2a944fd5c6bd0c6d8b1fcf70b4463fa7": {
      "translations": [
        {
          "translation": "Scammers can pretend to be your bank, using scare tactics to trick you into sending them money. Bitcoin Cash fixes this trust issue by removing third parties altogether."
        }
      ]
    },
    "d09ba9299507646e8d228883cedc134b": {
