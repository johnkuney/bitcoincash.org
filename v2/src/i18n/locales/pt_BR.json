    "3d65dbf9cddca9e877ad9fea43f29e2f": {
      "translations": [
        {
          "translation": "Pay privately"
        }
      ]
    },
    "1de5d67777a77620f0927972c8c58392": {
      "translations": [
        {
          "translation": "Since a bank account is tied to your identity, your transaction history is, too. Bitcoin Cash is the opposite: nobody knows who you are and payments can be sent privately."
        }
      ]
    },
    "53991ca9a39cc4478c77f393388aa482": {
      "translations": [
        {
          "translation": "Pay online without a bank account"
