    },
    "d4a3c5e1ce22110c4456a05a3e8b40de": {
      "translations": [
        {
          "translation": "Coffee to go? Time for a new laptop? Get it with Bitcoin Cash at an increasing number of participating websites and stores. Simply scan to check out, just like Google Pay."
        }
      ]
    },
    "2830fdff024a8df7092f940add52d98d": {
      "translations": [
        {
          "translation": "Send money abroad basically for free"
        }
      ]
    },
    "28fb543692f582d63dbf7a591ac93f5b": {
