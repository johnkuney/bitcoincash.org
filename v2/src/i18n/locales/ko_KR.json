    "7bf01da9afe61530d41015f688cdee3f": {
      "translations": [
        {
          "translation": "No matter how much you send, fees are always tiny."
        }
      ]
    },
    "4a38eb153d7be4a3cc8bda9f8341d3fe": {
      "translations": [
        {
          "translation": "Money is sent and received fast—no delays, no limits."
        }
      ]
