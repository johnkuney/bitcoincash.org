    "2cfcc5a7f9bcff424fc9b19fa98364a8": {
      "translations": [
        {
          "translation": "如何挣比特币现金？"
        }
      ]
    },
    "3039f47c3861beec0825d81ceee8c820": {
      "translations": [
        {
          "translation": "购买或挣得比特币现金"
        }
